{
  "name": "the-million-startups",
  "version": "1.0.0",
  "author": "Edwin Bartunek <edwin@devhart.io>",
  "license": "MIT",
  "keywords": [
    "sanity",
    "styled components",
    "framer motion",
    "next.js",
    "react"
  ],
  "private": true,
  "workspaces": [
    "studio"
  ],
  "scripts": {
    "dev": "concurrently --kill-others-on-fail -m 2  \"next\"  \"cp .env ./studio/.env.development && cd studio && sanity start\"",
    "dev:web": "next",
    "studio": "cp .env ./studio/.env.development && cd studio && sanity start",
    "build": "yarn run build:sanity && pwd && yarn run build:web",
    "build:web": "next build",
    "start": "next start",
    "build:sanity": "cd studio && sanity build ../public/studio -y && cd ..",
    "graphql": "cd studio && sanity graphql deploy && cd ..",
    "upgrade:studio": "cd studio && sanity upgrade && cd .. && yarn"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "2.0.7",
    "@sanity/client": "^2.2.6",
    "@sanity/image-url": "0.140.19",
    "axios": "^0.21.1",
    "classnames": "2.2.6",
    "concurrently": "^5.3.0",
    "date-fns": "2.15.0",
    "enhanced-resolve": "^5.7.0",
    "formik": "^2.2.6",
    "framer-motion": "^3.3.0",
    "get-video-id": "^3.1.9",
    "next": "10.0.5",
    "next-pwa": "3.1.5",
    "next-sanity": "^0.1.8",
    "node-mailjet": "^3.3.1",
    "react": "^17.0.1",
    "react-cookie-consent": "^6.2.2",
    "react-dom": "^17.0.1",
    "react-icons": "^4.1.0",
    "react-is": "^17.0.1",
    "react-player": "^2.8.2",
    "react-query": "^3.6.0",
    "styled-components": "^5.2.1",
    "swr": "^0.2.3",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@babel/core": "^7.12.13",
    "@sanity/cli": "^2.2.6",
    "babel-plugin-styled-components": "^1.12.0",
    "joi": "^17.4.0"
  },
  "repository": {
    "url": "https://github.com/millionstartups/million-startups-website"
  }
}

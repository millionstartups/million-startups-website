{
  "name": "the-million-startups",
  "version": "1.0.0",
  "author": "Edwin Bartunek <edwin@devhart.io>",
  "license": "MIT",
  "keywords": [
    "sanity",
    "styled components",
    "framer motion",
    "next.js",
    "react"
  ],
  "private": true,
  "workspaces": [
    "studio"
  ],
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"next\"  \"cp .env ./studio/.env.development && cd studio && sanity start\"",
    "dev:web": "next",
    "build": "yarn run build:sanity && pwd && yarn run build:web",
    "build:web": "next build",
    "start": "next start",
    "build:sanity": "cd studio && sanity build ../public/studio -y && cd ..",
    "studio": "cp .env ./studio/.env.development && cd studio && sanity start",
    "upgrade:studio": "cd studio && sanity upgrade && cd .. && yarn"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "2.0.7",
    "@sanity/client": "^2.1.4",
    "@sanity/image-url": "0.140.19",
    "@sanity/ui": "^0.32.7",
    "axios": "^0.21.1",
    "classnames": "2.2.6",
    "concurrently": "^5.3.0",
    "date-fns": "2.15.0",
    "formik": "^2.2.6",
    "framer-motion": "^3.2.2-rc.1",
    "next": "^10.0.5",
    "next-sanity": "^0.1.5",
    "react": "^17.0.1",
    "react-cookie-consent": "^6.2.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.7.0",
    "react-icons": "^4.1.0",
    "react-is": "^17.0.1",
    "styled-components": "^5.2.1",
    "swr": "^0.2.3",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@sanity/cli": "^2.1.4",
    "babel-plugin-styled-components": "^1.12.0",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.5.2"
  },
  "repository": {
    "url": "https://github.com/millionstartups/million-startups-website"
  }
}

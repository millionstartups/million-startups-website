{
  "name": "the-million-startups",
  "version": "1.0.0",
  "author": "Edwin Bartunek <edwin@devhart.io>",
  "license": "MIT",
  "keywords": [
    "sanity",
    "styled components",
    "framer motion",
    "next.js",
    "react"
  ],
  "scripts": {
    "dev": "next",
    "build": "npm run build:sanity && pwd && npm run build:web",
    "build:web": "next build",
    "start:web": "next start",
    "update:sanity": "cd studio && sanity upgrade && cd ..",
    "build:sanity": "cd studio && sanity build ../public/studio -y && cd ..",
    "start:sanity": "cp .env ./studio/.env.development && cd studio && sanity start",
    "postinstall": "lerna bootstrap"
  },
  "dependencies": {
    "@sanity/block-content-to-react": "2.0.7",
    "@sanity/client": "^2.1.4",
    "@sanity/image-url": "0.140.19",
    "axios": "^0.21.1",
    "classnames": "2.2.6",
    "date-fns": "2.15.0",
    "formik": "^2.2.6",
    "framer-motion": "^3.2.2-rc.1",
    "next": "^10.0.5",
    "next-sanity": "^0.1.5",
    "react": "^16.14.0",
    "react-dom": "^16.14.0",
    "react-hook-form": "^6.7.0",
    "styled-components": "^5.2.1",
    "swr": "^0.2.3",
    "yup": "^0.32.8"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.3.0",
    "@sanity/cli": "^2.1.4",
    "babel-plugin-styled-components": "^1.12.0",
    "lerna": "^3.22.1",
    "postcss-preset-env": "^6.7.0",
    "tailwindcss": "^1.5.2"
  },
  "repository": {
    "url": "https://github.com/millionstartups/million-startups-website"
  }
}
